package excercises;

public class part1 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		double average;
		double averageFinal;
		double[] menu = {23,6,47,35,2,14};
		average = menu[1] + menu[2] + menu[3] + menu[4] + menu[5] + menu[6];
		averageFinal = average / 6;
		System.out.println("The average of the array is: " + averageFinal);
	}

}
